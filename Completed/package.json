{
  "name": "Office-Add-in-NodeJS-SSO",
  "description": "",
  "author": "",
  "engines": {
    "node": ">=6.9.4"
  },
  "scripts": {
    "clean": "rimraf dist",
    "tsc": "tsc -p tsconfig.json",
    "tsc-w": "tsc -p tsconfig.json --watch",
    "cpx": "cpx \"./src/**/!(*.ts)\" dist",
    "cpx-w": "cpx \"./src/**/!(*.ts)\" dist --watch",
    "generate": "npm-run-all clean -p tsc cpx",
    "build": "npm-run-all clean -p tsc-w cpx-w",
    "start": "set NODE_ENV=development && nodemon dist/server.js --debug=5858"
  },
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.17.1",
    "cors": "2.8.3",
    "crypto-js": "3.1.9-1",
    "express": "4.15.2",
    "form-urlencoded": "1.5.0",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "7.3.0",
    "lodash": "4.17.4",
    "morgan": "1.8.1",
    "node-persist": "2.0.10",
    "nodemon": "1.11.0",
    "moment": "2.18.1",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.2",
    "@types/cors": "2.8.1",
    "@types/express": "4.0.35",
    "@types/jsonwebtoken": "7.2.0",
    "@types/lodash": "4.14.61",
    "@types/moment": "^2.13.0",
    "@types/morgan": "1.7.32",
    "@types/node-persist": "0.0.32",
    "@types/uuid": "2.0.29",
    "cpx": "1.5.0",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1",
    "typescript": "2.2.1"
  }
}
